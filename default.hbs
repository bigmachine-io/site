<!DOCTYPE html>
<html lang="{{@site.locale}}">
  <head>
    {{!-- Document Settings --}}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    {{!-- Page Meta --}}
    <title>{{meta_title}}</title>

    {{!-- Mobile Meta --}}
    <meta name="HandheldFriendly" content="True" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    
    {{!-- PWA --}}
    {{!-- <meta name="theme-color" content="{{@site.accent_color}}">
    <link rel="manifest" href="/manifest.webmanifest">
    <link rel="apple-touch-icon" href="{{asset 'icon-192x192.png'}}"> --}}

    {{!-- Preloading --}}
    <link rel="preload" href="{{asset 'dist/app.min.js'}}" as="script">
    <link rel="preload" href="{{asset 'dist/app.min.css'}}" as="style">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    {{#if @custom.typography}}
      {{#match @custom.typography "Inter"}}
        <link rel="preload stylesheet" href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&display=swap" 
              as="style" onload="this.onload=null;this.rel='stylesheet'" crossorigin>

        {{!-- No Js --}}
        <noscript>
          <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
        </noscript>
      {{/match}}

      {{!-- Cabin --}}
      {{#match @custom.typography "Cabin"}}
        <link rel="preload stylesheet" href="https://fonts.googleapis.com/css2?family=Cabin:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&display=swap" 
              as="style" onload="this.onload=null;this.rel='stylesheet'" crossorigin>
        <noscript>
          <link href="https://fonts.googleapis.com/css2?family=Cabin:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">
        </noscript>
        <style>
          body {
            --font-sans: 'Cabin';
          }

        </style>
      {{/match}}

      {{!-- Lora --}}
      {{#match @custom.typography "Lora"}}
        <link rel="preload stylesheet" href="https://fonts.googleapis.com/css2?family=Lora:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&display=swap" 
              as="style" onload="this.onload=null;this.rel='stylesheet'" crossorigin>
        <noscript>
          <link href="https://fonts.googleapis.com/css2?family=Lora:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">
        </noscript>
        <style>
          body {
            --font-sans: 'Lora';
          }
        </style>
      {{/match}}

      {{!-- Manrope --}}
      {{#match @custom.typography "Manrope"}}
        <link rel="preload stylesheet" href="https://fonts.googleapis.com/css2?family=Manrope:wght@400;500;600;700;800&display=swap" 
              as="style" onload="this.onload=null;this.rel='stylesheet'" crossorigin>
        <noscript>
          <link href="https://fonts.googleapis.com/css2?family=Manrope:wght@400;500;600;700;800&display=swap" rel="stylesheet">
        </noscript>
        <style>
          body {
            --font-sans: 'Manrope';
          }
        </style>
      {{/match}}

      {{!-- Inconsolata --}}
      {{#match @custom.typography "Inconsolata"}}
        <link rel="preload stylesheet" href="https://fonts.googleapis.com/css2?family=Inconsolata:wght@400;500;600;700;800;900&display=swap" 
              as="style" onload="this.onload=null;this.rel='stylesheet'" crossorigin>
        <noscript>
          <link href="https://fonts.googleapis.com/css2?family=Inconsolata:wght@400;500;600;700;800;900&display=swap" rel="stylesheet">
        </noscript>
        <style>
          body {
            --font-sans: 'Inconsolata';
          }
        </style>
      {{/match}}
    {{else}}
      <link rel="preload stylesheet" href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&display=swap" 
            as="style" onload="this.onload=null;this.rel='stylesheet'" crossorigin>

      {{!-- No Js --}}
      <noscript>
        <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
      </noscript>
    {{/if}}

    {{!-- Main theme scripts --}}
    <script async defer src="{{asset 'dist/app.min.js'}}"></script>

    {{!-- Styles --}}
    <link rel="stylesheet" type="text/css" href="{{asset 'dist/app.min.css'}}" />
    <link rel="stylesheet" type="text/css" src="{{asset 'css/nightowl.css'}}"/>
    <link rel="stylesheet" type="text/css" src="{{asset 'css/custom.css'}}"/>
    {{!-- Critical CSS --}}
    <!--cssential--><style></style><!--/cssential-->
<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
<style>
  h1,h2{
    text-transform: uppercase
  }
.homes {
background: rgb(2,0,36);
background: radial-gradient(circle, rgba(2,0,36,1) 0%, rgba(43,46,83,0.9402354691876751) 0%, rgba(20,20,20,1) 16%);
}

.gradient {
  background: linear-gradient(90deg, #1A202C 0%, #A0AEC0 100%);
}
.graded{
   background: #ECE9E6;  /* fallback for old browsers */
  background: -webkit-linear-gradient(to right, #FFFFFF, #ECE9E6);  /* Chrome 10-25, Safari 5.1-6 */
  background: linear-gradient(to right, #FFFFFF, #ECE9E6); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */

 }
.bluesy{

  background: #ECE9E6;
  background: -webkit-linear-gradient(to right, #FFFFFF, rgb(207, 239, 255));
  background: linear-gradient(to left, rgb(236, 253, 253), rgb(207, 239, 255));
}
.blue-striped {
  box-shadow: 0 13px 27px -5px rgba(30,132,242,0.58), 0 8px 16px -8px rgba(30,132,242,0.84), 0 -6px 16px -6px rgba(30,132,242,1)
}
.striped {
  box-shadow: 0 13px 27px -5px rgba(50,50,93,.25), 0 8px 16px -8px rgba(0,0,0,.3), 0 -6px 16px -6px rgba(0,0,0,.025)
}

.electric {
background-color: #4158D0;
background-image: linear-gradient(43deg, #4158D0 0%, #C850C0 46%, #FFCC70 100%);

}
.morning{
  background: #004FF9;  /* fallback for old browsers */
background: -webkit-linear-gradient(to top, #FFF94C, #004FF9);  /* Chrome 10-25, Safari 5.1-6 */
background: linear-gradient(to top, #FFF94C, #004FF9); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */

}
.sunset{
  background: #d34a3b;  /* fallback for old browsers */
background: -webkit-linear-gradient(to top, #ffc500, #c21500);  /* Chrome 10-25, Safari 5.1-6 */
background: linear-gradient(to top, #ffc500, #c21500); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */

}
.footer__top{
  background-color: transparent;
}
.footer__top > li > a, .footer__top > p > a{
  background-color: transparent;
  color: #000
}
.footer .nav a {
  color: #ccc
}
.footer__description, .footer_bottom  a {
  color: #fff;
  font-weight: bold;
}
</style>

    {{!-- Theme Configuration --}}
    {{> theme-config}}

    {{!-- Global scripts for setting values --}}
    {{> theme-scripts}}
    
    {{!-- Syntax highlighting plugin - can be activated using #syntax-highlight internal tag --}}
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.14.0/prism.min.js"></script>

    {{!-- Ghost outputs important style and meta data with this tag --}}
    {{ghost_head}}
  </head>
  <body class="{{body_class}}">
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-M6PK3T"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->


    {{!-- Announcement --}}
    {{> announcement}}
    
    {{!-- Progress Bar for posts --}}
    {{#is "post"}}<div class="progress-bar"></div>{{/is}}
    
    {{!-- Site header --}}
    {{> header}}

    {{!-- Site Menu --}}
    {{> menu}}

    {{!-- Site Search --}}
    {{> search}}

    <main class="container wrapper main{{#is "signin, signup"}} flex{{/is}}">
      {{!-- All the main content gets inserted here, index.hbs, post.hbs, etc --}}
      {{{body}}}
    </main>

    {{!-- The footer at the very bottom --}}
    {{> footer}}

    {{!-- Scroll top function --}}
    {{> scroll-top}}
  
    {{{block "toc"}}}

    {{!-- Notifications --}}
    {{> notifications}}

    {{!-- Service Worker for PWA --}}
    <script>
      if (CONFIG.ENABLE_PWA && 'serviceWorker' in navigator) {
        window.addEventListener('load', function() {
          navigator.serviceWorker.register('/sw.js');
        });
      }
    </script>

    {{!-- The #block helper will pull in data from the #contentFor other template files. --}}
    {{{block "scripts"}}}

    {{!-- Ghost outputs important scripts and data with this tag --}}
    {{ghost_foot}}



  </body>
</html>
